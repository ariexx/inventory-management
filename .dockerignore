# Abaikan semua file dan folder yang terkait dengan Git
.git
.gitignore

# Abaikan direktori dependensi lokal
# Ini akan di-install di dalam kontainer
/vendor
/node_modules

# Abaikan file konfigurasi lokal dan rahasia
.env
.env.example
.env.testing

# Abaikan file spesifik IDE atau OS
.idea
.vscode
*.DS_Store
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Abaikan file dokumentasi atau kontribusi
README.md
CONTRIBUTING.md

# Abaikan konfigurasi build lokal
docker-compose.yml
Dockerfile
.dockerignore

# Abaikan file storage lokal Laravel, kecuali .gitignore
/storage/*
!/storage/.gitignore
/storage/app/public/*
!/storage/app/public/.gitignore

# Abaikan file log
/storage/logs/*.log
/public/hot
